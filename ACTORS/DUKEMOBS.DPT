    TNT1 A 0
    TNT1 A 0 A_GiveInventory("ExpandedDeathCheck",1)
    TNT1 A 1 A_Die
    stop
		Crush:
		TNT1 AAAAAAAAAA 0 A_CustomMissile ("BloodTr255", 0, 0, random (0, 360), 2, random (0, 360))
