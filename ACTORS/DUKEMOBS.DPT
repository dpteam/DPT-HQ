  WoundDeath:
    ENE1 N 7 A_Scream
    ENE1 L 7
    ENE1 M -1 A_NoBlocking
    stop
  DeathCause:
    ENE1 AABB 3 A_Wander
    ENE1 A 1 A_Die
    stop
  Death.Shrinker:
    ENE1 A 0
    ENE1 A 0 A_PlaySound("shrinker/shrink")
    ENE1 A 20 A_Pain
    ENE1 A 0 A_CustomMissile("ShrinkEffect",8,0,0)
    ENE1 M 0 
    ENE1 A 0 A_SpawnItemEx("Alien_GreenShrunk",0,0,0,0,0,0,0,32,0)
