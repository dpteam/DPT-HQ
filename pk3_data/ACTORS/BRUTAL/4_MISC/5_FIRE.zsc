CLASS MANCUBIFIRE : ACTOR { 
	Default {
		
		RADIUS 1;
		HEIGHT 1;
		ALPHA 0.3;
		RENDERSTYLE "Translucent";
		SCALE 0.6;
		SPEED 2;
		GRAVITY 0;
		+NOBLOCKMAP;
		+NOTELEPORT;
		+DONTSPLASH;
		+MISSILE;
		+FORCEXYBILLBOARD;
		+CLIENTSIDEONLY;
	}
	STATES
	{
		SPAWN:
			TNT1 A 1;
			GOTO DEATH;
		DEATH:
			FX98 ABCDEFGHIJKL 2 BRIGHT A_SPAWNITEM("YELLOWFLARE",0,0);
			EXPL A 0 A_CUSTOMMISSILE ("EXPLOSIONSMOKE", 0, 0, RANDOM (0, 360), 2, RANDOM (0, 360));
			EXPL A 0 A_CUSTOMMISSILE ("BIGSMOKES", 0, 0, RANDOM (0, 360), 2, RANDOM (0, 360));
			FX98 MNOPQ 2;
			STOP;
	}
}
