CLASS FLAMETRAILS : ACTOR { 
	Default {
		
		RADIUS 1;
		HEIGHT 1;
		SPEED 3;
		DAMAGE 0;
		+NOBLOCKMAP;
		+NOTELEPORT;
		+DONTSPLASH;
		+MISSILE;
		+FORCEXYBILLBOARD;
		+CLIENTSIDEONLY;
		+NOINTERACTION;
		+NOCLIP;
		RENDERSTYLE "Add";
		DAMAGETYPE FLAMES;
		SCALE 0.5;
		ALPHA 1;
		GRAVITY 0;
	}
	STATES
	{
		SPAWN:
			TNT1 A 2;
			FRPR ABCDEFGH 3 BRIGHT;
			STOP;
	}
}
