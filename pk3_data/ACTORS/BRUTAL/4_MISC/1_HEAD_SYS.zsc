			TNT1 A 1 A_SpawnITEM ("HEADSHOTSSG", 0, 0);
			TNT1 A 1;
			Stop;
		DEATH.TAUNT:
			TNT1 A 1 A_SpawnITEM ("HEADSHOTHITTAUNT", 0, 0);
			TNT1 A 1;
			Stop;
