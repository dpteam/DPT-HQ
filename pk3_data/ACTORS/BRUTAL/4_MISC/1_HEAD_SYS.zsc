		+GHOST;
		+SERVERSIDEONLY;
		-COUNTKILL;
		DAMAGEFactor "TRAMPLE", 0;
	}
	States
	{
		Spawn:
			XXXX A 5;
			XXXX A 1;
			Stop;
		PAIN:
			TNT1 A 4;
			TNT1 A 0 A_SpawnITEMEX ("HEADSHOTMINORHIT");
			TNT1 A 1;
			Stop;
