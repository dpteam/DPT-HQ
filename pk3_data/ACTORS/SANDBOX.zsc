// Arachnotron
class "Sandbox-ArachnotronEnemySpawner" : "Sandbox-EnemySpawnerModule" { 
	Default {
		
		Inventory.Icon "BSPIICON";
	}
	States
	{
		Spawn:
			DOOM A 1;
			Loop;
		Use:
			TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(101), 2);
			TNT1 A 0 A_GiveInventory("Sandbox-ArachnotronEnemySpawner");
			Stop;
			DOOM A 1 A_FireCustomMissile("Sandbox-ArachnotronEnemySpawner.Projectile", 1, 0, 0, 0);
			Stop;
	}
}
class "Sandbox-ArachnotronEnemySpawner.Projectile" : actor { 
	Default {
		
		Radius 4;
		Height 4;
		Speed 4;
		Damage 0;
		Scale 1.0;
		+NoBlockmap;
		+ActivatePCross;
		+ActivateImpact;
		-NoTeleport;
		+DropOff;
		+DontFall;
		+Missile;
		+DOOMBOUNCE;
		+BOUNCEONactorS;
		DeathSound "misc/teleport";
	}
	States
	{
		Spawn:
			DOOM A 1 Bright;
			Loop;
		Death:
			TFOG A 8 Bright ACS_ExecuteAlways(102, 0, 0);
			TFOG BCDEFGHIJ 8 Bright;
			Stop;
	}
}
class "Sandbox-ArachnotronEnemy" : Arachnotron { //replaces Arachnotron
	Default {
		
		-SOLID;
	}
}
