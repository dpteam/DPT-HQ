	Default {
		
		Inventory.Icon "FATTICON";
	}
	States
	{
		Spawn:
			DOOM A 1;
			Loop;
		Use:
			TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(101), 2);
			TNT1 A 0 A_GiveInventory("Sandbox-FatsoFriendSpawner");
			Stop;
			DOOM A 1 A_FireCustomMissile("Sandbox-FatsoFriendSpawner.Projectile", 1, 0, 0, 0);
			Stop;
	}
}
class "Sandbox-FatsoFriendSpawner.Projectile" : actor { 
	Default {
		
		Radius 4;
		Height 4;
		Speed 4;
		Damage 0;
		Scale 1.0;
		+NoBlockmap;
		+ActivatePCross;
		+ActivateImpact;
		-NoTeleport;
		+DropOff;
		+DontFall;
		+Missile;
		+DOOMBOUNCE;
		+BOUNCEONactorS;
		DeathSound "misc/teleport";
	}
	States
	{
		Spawn:
			DOOM A 1 Bright;
			Loop;
		Death:
			TFOG A 8 Bright ACS_ExecuteAlways(100, 0, 10);
			TFOG BCDEFGHIJ 8 Bright;
			Stop;
	}
}
class "Sandbox-FatsoFriend" : Fatso { 
	Default {
		
		+FRIENDLY;
		-SOLID;
	}
}
