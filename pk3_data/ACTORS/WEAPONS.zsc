		ASSU A 0 A_SetPitch(pitch-4.5);
		Goto PostFire;
	Fire1:
		ASSU A 0 A_FireBullets (1, 1, -1, 13, "NoGibPuff");
		ASSU A 0 A_SetPitch(pitch-4.5);
		Goto PostFire;
	PostFire:
		ASSU A 0 A_PlayweaponSound("m16/Fire");
