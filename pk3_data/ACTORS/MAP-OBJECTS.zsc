			BEXP A 0 A_SPAWNITEMEX ("BARRELKABOOM",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
			BEXP AAAAAAA 0 A_CUSTOMMISSILE ("EXPLOSIONFLAMES", 0, 0, RANDOM (0, 360), 2, RANDOM (0, 360));
			BEXP AAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_CUSTOMMISSILE ("EXPLOSIONPARTICLEHEAVY", 0, 0, RANDOM (0, 360), 2, RANDOM (0, 180));
			BEXP AAAAAA 0 A_CUSTOMMISSILE ("EXPLOSIONPARTICLEHEAVY", 0, 0, RANDOM (0, 360), 2, RANDOM (0, 180));
			BEXP A 0 RADIUS_QUAKE (2, 24, 0, 15, 0);
			BEXP A 0 Radius_Quake(3,15,0,5,0);
			BEXP B 0 BRIGHT A_SCREAM();
			BEXP AA 0 A_CUSTOMMISSILE ("METALSHARD1", 5, 0, RANDOM (0, 360), 2, RANDOM (30, 160));
			BEXP AA 0 A_CUSTOMMISSILE ("METALSHARD2", 5, 0, RANDOM (0, 360), 2, RANDOM (30, 160));
			BEXP AA 0 A_CUSTOMMISSILE ("METALSHARD3", 5, 0, RANDOM (0, 360), 2, RANDOM (30, 160));
			BEXP A 0 A_PLAYSOUND("FAREXPL", 3);
			BEXP A 0 A_SPAWNITEM("BARRELEXPLOSIONSMOKECOLUMN");
			BEXP AAAAAAA 0 A_custommissile("SmokingPiece22",random(0,16),random(-8,8),random(-360,360),2,random(10,70));
			BEXP AAAAAA 0 A_custommissile("BarrelPart",20,random(-5,5),random(0,360),2,random(40,80));
			BEXP A 0 A_SpawnItem("XBarrelChunk",0,0);
			BEXP AAAAA 8 A_CUSTOMMISSILE ("EXPLOSIONSMOKE", 1, 0, RANDOM (0, 360), 2, RANDOM (50, 130));
