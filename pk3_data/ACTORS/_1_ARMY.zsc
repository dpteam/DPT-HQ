			TNK1 B 2 A_Recoil(-1);
			TNT1 A 0 A_SpawnItem("TreadsDamage",65);
			TNK1 D 2 A_Recoil(-1);
			TNT1 A 0 A_SpawnItemEx ("ZombieMainBattleTankBody1", 0, 0, 0, velx, vely, velz, angle, SXF_ABSOLUTEVELOCITY | SXF_ABSOLUTEANGLE);
